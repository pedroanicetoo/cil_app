<!DOCTYPE html>
<html>
<head>
  <title>Ciliatest</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "site/home", :media => "all" %>
  <%= stylesheet_link_tag    "site/orders", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= render partial: 'layouts/shared/notifications' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="d-flex" id="wrapper">
  <%= stylesheet_link_tag    "site/home", :media => "all" %>
  <%= javascript_include_tag "site/home" %>
  <% if client_signed_in? %>
    <%= render partial: 'layouts/shared/sidebar', locals: {action: "Editar Administrador"} %>
  <% end  %>
    <!-- Page Content -->
    <div id="page-content-wrapper">
      <%= render partial: 'layouts/shared/navigation', locals: {action: "Editar Administrador"} %>
      <%= yield %>
    </div>
    <!-- /#page-content-wrapper -->
  </div>
</body>
</html>
